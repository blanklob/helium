{
  "name": "shopify-webpack-with-os2-features",
  "version": "1.0.3",
  "main": "theme.js",
  "description": "Modern development on Shopify using Webpack 5 fully supporting the new Shopify Theme CLI and Online Store 2.0 features.",
  "repository": "https://github.com/younessidbakkasse/shopify-webpack-with-os2-features.git",
  "author": "Youness Id bakkasse <std.youness@gmail.com>",
  "license": "MIT",
  "keywords": [
    "boilerplate",
    "webpack",
    "typescript",
    "shopify",
    "sass"
  ],
  "prettierIgnore": [
    "assets"
  ],
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "shopify theme serve",
    "build": "webpack --config webpack.config.prod.js",
    "watch": "webpack watch --config webpack.config.dev.js",
    "zip": "shopify theme package .",
    "lint": "shopify theme check -a && lit-analyzer",
    "format": "prettier --write .",
    "pull": "shopify theme pull -n",
    "push": "shopify theme push",
    "stats": "webpack --profile --json > stats.json"
  },
  "devDependencies": {
    "css-loader": "^5.2.6",
    "media-query-plugin": "^1.5.0",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "ts-lit-plugin": "^1.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0",
    "webpack-remove-empty-scripts": "^0.7.1"
  },
  "dependencies": {}
}
