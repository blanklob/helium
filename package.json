{
  "name": "shopify-webpack-with-os2-features",
  "version": "1.0.0",
  "main": "theme.js",
  "repository": "https://github.com/younessidbakkasse/shopify-webpack-with-os2-features.git",
  "author": "Youness Id bakkasse <std.youness@gmail.com>",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "concurrently \"yarn watch\" \"yarn serve\"",
    "build": "webpack",
    "watch": "webpack watch",
    "serve": "cd dist && shopify theme serve dist ",
    "shopify:zip": "shopify theme package dist",
    "lint:format": "prettier --write src",
    "lint:theme": "shopify theme check",
    "lint:scripts": " eslint src/**/*.js",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "git:log": "git log --oneline --decorate --graph",
    "git:push-prod": "git subtree push --prefix dist origin prod",
    "git:push-dev": "git push origin dev"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "core-js": "^3.15.2",
    "normalize.css": "^8.0.1"
  }
}
