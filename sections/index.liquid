<div class="s-index" data-section-id="{{- section.id -}}">
  <div class="o-container o-container--s u-align-center" data-component-example>
    <div class="s-index__logo u-mb-md">{%- render 'tools-icons' icon: 'shopify', icon_size: 120 -%}</div>
    {% comment %} Heading injected by JS {% endcomment %}
    <h2 class="s-index__heading u-mb-s" data-component-example="heading"></h2>
    <p class="s-index__subheading u-mb-l" data-component-example="subheading"></p>
    <a class="s-index__link c-btn c-btn--primary"
      href="https://github.com/younessidbakkasse/shopify-webpack-with-os2-features"
      target="_blank"> Check Github Repo
    </a>
  </div>
</div>


{% schema %}
{
  "name": "Index",
  "tag": "section",
  "settings": []
}
{% endschema %}
